{
	"manifest_version": 2,
	"name": "FUTURE Search",
	"short_name": "future_search",
	"description": "Adds wearebuildingthefuture.com as a search engine.",
	"version": "1.0.0",
	"icons": {
		"128": "icons/128x128.png"
	},
	"chrome_settings_overrides": {
		"search_provider": {
			"favicon_url": "https://wearebuildingthefuture/favicon.ico",
			"is_default": true,
			"keyword": "@ftr",
			"name": "FUTURE Search",
			"search_url": "https://wearebuildingthefuture.com/",
			"search_url_get_params": "q={searchTerms}",
			"suggest_url": "https://wearebuildingthefuture.com/_autocomplete/",
			"suggest_url_get_params": "term={searchTerms}&type=list"
		}
	}
}
